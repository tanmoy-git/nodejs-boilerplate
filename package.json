{
  "name": "nodejs-boilerplate",
  "version": "1.0.0",
  "engines": {
    "node": "12.4.0"
  },
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "nodemon -e ts index.js",
    "start:debug": "nodemon -e ts --inspect index.js",
    "clean": "rm -fr node_modules && rm -fr package-lock.json && npm install",
    "build": "rm -fr dist && tsc",
    "ci-build": "tsc",
    "ci-build-production": "npm i --production && npm i --no-save typescript && tsc && npm uninstall --no-save @evojam/tslint-rules tslint typescript",
    "ci-verify-tslint": "tslint -p .",
    "test": "npm run build && tslint -p ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "verbose": true,
    "delay": "500",
    "watch": [
      "./src"
    ]
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "@nestjs/common": "6.3.0",
    "@nestjs/core": "6.0.0",
    "@nestjs/platform-express": "6.0.0",
    "@nestjs/swagger": "3.0.2",
    "@types/jsonwebtoken": "8.3.2",
    "@types/uuid": "3.4.4",
    "axios": "0.19.0",
    "class-transformer": "0.2.0",
    "class-validator": "0.9.1",
    "dotenv": "7.0.0",
    "http-status-codes": "1.3.2",
    "immutable": "4.0.0-rc.12",
    "jsonstream": "1.0.3",
    "jsonwebtoken": "8.5.1",
    "module-alias": "2.2.0",
    "moment": "2.29.4",
    "node-schedule": "1.3.2",
    "reflect-metadata": "0.1.12",
    "request": "2.88.0",
    "rxjs": "6.4.0",
    "swagger-ui-express": "4.0.3",
    "tsyringe": "3.2.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@evojam/tslint-rules": "github:evojam/tslint-rules",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/jsonstream": "0.8.30",
    "@types/module-alias": "2.0.0",
    "@types/node": "11.13.4",
    "@types/axios": "0.14.0",
    "nodemon": "1.19.0",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "typescript": "3.4.3"
  },
  "typesVersions": {
    ">=3.1": {
      "*": [
        "ts3.1/*"
      ]
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "dist",
      "**/*.d.ts",
      "src/test",
      "src/migrations",
      "coverage"
    ],
    "reporter": [
      "text"
    ],
    "all": true,
    "require": [
      "ts-node/register"
    ]
  }
}
